
server {
    listen 127.0.0.1:8080;
    listen 127.0.0.1:8084;
    listen 127.0.0.1:8085;
    server_name localhost;
    root /Users/mait-aad/Desktop/;
    error_page 404 /404.html;
    error_page 500 502 503 504 /50x.html;

    location / {
        autoindex ON;
        root /Users/mait-aad/Desktop/HTML_All-in-one_School-Website/;
        error_page 500 502 503 504 /50x.html;
        try_files $uri $uri/ /index.html;
        try_files $uri $uri/ /index.html;
    }
    location /Users/mait-aad/Desktop/HTML_All-in-one_School-Website/ {
        try_files $uri $uri/ /index.html;
        error_page 500 502 503 504 /50x.html;
        try_files $uri $uri/ /index.html;
        try_files $uri $uri/ /index.html;
    }
    location / {
        root /Users/mait-aad/Desktop/HTML_All-in-one_School-Website/;
        try_files $uri $uri/ /index.html;
        error_page 500 502 503 504 /50x.html;
        try_files $uri $uri/ /index.html;
        try_files $uri $uri/ /index.html;
    }
    location /ali/mohamed {
        try_files $uri $uri2/ /index.html;
        try_files $uri $uri2/ /index.html;
        try_files $uri $uri2/ /index.html;
    }
    client_max_body_size 10m;
}

