
server {
    listen 127.0.0.1;
    listen 127.0.0.1:8084;
    listen 127.0.0.1:8085;
    server_name localhost;
    autoindex ON;

    error_page 404 /404.html;
    error_page 500 502 503 504 /50x.html;

    location / {
        methods GET;
        autoindex ON;
        try_files $uri $uri/ /index.html;
        root /Users/omar/Desktop/webserver/;
        error_page 500 502 503 504 /50x.html;
        try_files $uri $uri/ /index.html;
        try_files $uri $uri/ /index.html;
        cgi_path php;
    }
    location /tt {
        index index.py index.php index.html;
        methods GET;
        autoindex ON;
        try_files $uri $uri/ /index.html;
        root /Users/omar/Desktop/maroc/;
        error_page 500 502 503 504 /50x.html;
        try_files $uri $uri/ /index.html;
        try_files $uri $uri/ /index.html;
        return /redi;
    }
    location /redi {
        methods GET;
        autoindex ON;
        try_files $uri $uri/ /index.html;
        root /Users/omar/Desktop/mac-sort/;
        error_page 500 502 503 504 /50x.html;
        try_files $uri $uri/ /index.html;
        try_files $uri $uri/ /index.html;
    }
    location /upload {
        autoindex ON;
        methods POST;
        upload ON;
        root /Users/omar/Desktop/webserver/UP;
        try_files $uri $uri/ /index.html;
        error_page 500 502 503 504 /50x.html;
        try_files $uri $uri/ /index.html;
        try_files $uri $uri/ /index.html;
    }
    location /cgi-bin/ {
        autoindex ON;
        methods POST GET;
        cgi_path py;
        root /Users/omar/Desktop/webserver/;
        try_files $uri $uri/ /index.html;
        error_page 500 502 503 504 /50x.html;
        try_files $uri $uri/ /index.html;
        try_files $uri $uri/ /index.html;
    }
    client_max_body_size 10m;
}

server {
    listen 127.0.0.1:8082;
    server_name localhost;

    root /Users/omar/Desktop;
    root /Users/omar/Desktop/webserver;
    autoindex ON;
    error_page 404 /404.html;
    error_page 500 502 503 504 /50x.html;

    location / {
        try_files $uri $uri/ /index.html;
        error_page 500 502 503 504 /50x.html;
        try_files $uri $uri/ /index.html;
        try_files $uri $uri/ /index.html;
    }
    location /Users/omar/Desktop/webserver/ {
        try_files $uri $uri/ /index.html;
        error_page 500 502 503 504 /50x.html;
        try_files $uri $uri/ /index.html;
        try_files $uri $uri/ /index.html;
    }
    location /dsfdsf/dsfdsf/dsf {
        try_files $uri $uri/ /index.html;
        error_page 500 502 503 504 /50x.html;
        try_files $uri $uri/ /index.html;
        try_files $uri $uri/ /index.html;
    }
    location /ali/mohamed {
        try_files $uri $uri2/ /index.html;
        try_files $uri $uri2/ /index.html;
        try_files $uri $uri2/ /index.html;
    }
    client_max_body_size 10m;
}
